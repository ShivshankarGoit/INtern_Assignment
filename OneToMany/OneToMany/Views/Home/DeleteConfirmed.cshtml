@model object  // Use 'object' for flexibility in handling both StudentDetail and course



<h2>Delete Confirmation</h2>

<p>Are you sure you want to delete this record?</p>

@if ( Model is OneToMany.Models.StudentDetail)
{
    var student = Model as OneToMany.Models.StudentDetail;
    <dl class="dl-horizontal">
        <dt>Student ID</dt>
        <dd>@student.student_id</dd>
        <dt>Student Name</dt>
        <dd>@student.student_name</dd>
    </dl>
}
else if (Model is OneToMany.Models.course)
{
    var course = Model as OneToMany.Models.course;
    <dl class="dl-horizontal">
        <dt>Course ID</dt>
        <dd>@course.course_id</dd>
        <dt>Course Name</dt>
        <dd>@course.course_name</dd>
        <dt>Student ID</dt>
        <dd>@course.student_id</dd>
    </dl>
}

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <p>
        <input type="submit" value="Delete" class="btn btn-danger" />
        @Html.ActionLink("Cancel", "Index", null, new { @class = "btn btn-default" })
    </p>
}


