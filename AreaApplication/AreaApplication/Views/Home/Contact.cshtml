@model IEnumerable<AreaApplication.Models.excellSheet>

@{
    ViewBag.Title = "Exportexcell";
}


<div class="container">
    <table class="table table-bordered table-hover">
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>City</th>

        </tr>

        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.Id</td>
                <td>@employee.Name</td>
                <td>@employee.City</td>

            </tr>
        }
    </table>

    <a href="@Url.Action("ExportToExcel","Database")" class="btn btn-primary">Export to Excel</a>

</div>
