<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>

    <link href="@Url.Content("~/Content/img/favicon.png")" rel="icon">
    <link href="@Url.Content("~/Content/img/apple-touch-icon.png")" rel="apple-touch-icon">
    <!-- Fonts -->
    <link href="https://fonts.googleapis.com" rel="preconnect">
    <link href="https://fonts.gstatic.com" rel="preconnect" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,700;1,900&family=Lato:ital,wght@0,100;0,300;0,400;0,700;0,900;1,100;1,300;1,400;1,700;1,900&display=swap" rel="stylesheet">

    <!-- Vendor CSS Files -->
    <link href="@Url.Content("~/Content/vendor/bootstrap/css/bootstrap.min.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/vendor/bootstrap-icons/bootstrap-icons.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/vendor/aos/aos.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/vendor/swiper/swiper-bundle.min.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/vendor/glightbox/css/glightbox.min.css")" rel="stylesheet">

    @*@Styles.Render("~/Content/css")*@
    <!-- Main CSS File -->

    <link href="@Url.Content("~/Content/css/main.css")" rel="stylesheet">
    @Scripts.Render("~/bundles/modernizr")


</head>
<body class="index-page">
    
    <div class="container body-content">
        @Html.Partial("_Header")
        
        @RenderBody()
        
        @Html.Partial("_Footer")

    </div>
  
    <!-- Main JS File -->

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
